    "axios": "^1.6.2",
    "cheerio": "^1.1.2",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "gender-detection-from-name": "^1.11.4",
    "helmet": "^7.1.0",
