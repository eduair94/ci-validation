{
  "name": "ci-validation",
  "version": "1.1.9",
  "description": "üá∫üáæ Complete TypeScript/JavaScript library for validating Uruguayan CI (C√©dula de Identidad) with official algorithm and government service integration",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "bin": {
    "ci-validate": "dist/cli/index.js"
  },
  "exports": {
    ".": {
      "import": "./dist/lib/index.js",
      "require": "./dist/lib/index.js",
      "types": "./dist/lib/index.d.ts"
    }
  },
  "scripts": {
    "test_smi": "ts-node src/services/Smi.test.ts",
    "test_smi_api": "ts-node -e \"import('./api/smi').then(m => console.log('SMI API endpoint compiled successfully'))\"",
    "test_smi_proxy": "ts-node src/test-smi-proxy.ts",
    "test_new_ci_service": "ts-node src/test_new_ci_service.ts",
    "dev": "nodemon src/index.ts",
    "build": "tsc && npm run build:cli && npm run build:npm",
    "build:cli": "echo '#!/usr/bin/env node' > dist/cli/cli.js && cat dist/cli/cli-main.js >> dist/cli/cli.js && chmod +x dist/cli/cli.js",
    "build:npm": "cp NPM_README.md dist/README.md",
    "start": "node dist/index.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npm run build",
    "publish:dry": "npm publish --dry-run",
    "publish:npm": "npm publish --access public",
    "docs": "node dist/scripts/generate-swagger.js"
  },
  "files": [
    "dist",
    "LICENSE",
    "package.json"
  ],
  "keywords": [
    "uruguay",
    "uruguayan",
    "cedula",
    "c√©dula",
    "ci",
    "validation",
    "validator",
    "identity",
    "document",
