    "government",
    "official",
    "algorithm",
    "check-digit",
    "latin-america",
    "south-america"
  ],
  "author": "Eduardo Airaudo <eduair94@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/eduair94/ci-validation.git"
  },
  "bugs": {
    "url": "https://github.com/eduair94/ci-validation/issues"
  },
  "homepage": "https://github.com/eduair94/ci-validation#readme",
