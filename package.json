    "redis": "^5.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^6.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
